{
  "name": "@wethegit/components",
  "version": "2.3.1",
  "main": "./src/index.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "./src/**/*",
    "./src/**/!(*.stories.*)",
    "./src/**/!(.storybook)"
  ],
  "type": "module",
  "exports": {
    "./components/*": "./src/components/*",
    "./hooks/*": "./src/hooks/*",
    "./utilities/*": "./src/utilities/*",
    "./types/*": "./src/types/*",
    "./styles/*": "./src/styles/*",
    ".": "./src/index.ts"
  },
  "scripts": {
    "build": "storybook build --docs",
    "clean": "rm -rf ./.turbo ./node_modules ./dist ./storybook-static",
    "start": "storybook dev -p 6006",
    "lint": "eslint src/",
    "stylelint": "stylelint **/*.{css,scss}",
    "preview-storybook": "serve storybook-static -l 6006",
    "test-storybook": "test-storybook",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@wethegit/react-hooks": "^2.0.0",
    "@wethegit/react-modal": "^2.0.0",
    "react": "17 - 18",
    "react-dom": "17 - 18",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-docs": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-themes": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/jest": "^0.2.3",
    "@storybook/manager-api": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-vite": "^7.5.3",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^7.5.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.3.2",
    "@wethegit/react-hooks": "^2.0.0",
    "@wethegit/react-modal": "^2.0.0",
    "eslint": "^8.15.0",
    "eslint-config-custom": "*",
    "eslint-plugin-storybook": "^0.6.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.69.5",
    "serve": "^13.0.4",
    "storybook": "^7.5.3",
    "stylelint": "^15.11.0",
    "stylelint-config-custom": "*",
    "tsconfig": "*",
    "typescript": "5.1.6",
    "vite": "^4.4.9",
    "wait-on": "^7.2.0",
    "lint-staged-config": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
